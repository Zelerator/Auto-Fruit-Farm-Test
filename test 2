spawn(function()
while _G.Rocket == true do
    local args = {
        [1] = "StoreFruit",
        [2] = "Rocket-Rocket",
        [3] = game:GetService("Players").LocalPlayer.Character:FindFirstChild("Rocket Fruit")
    }
    
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
    wait()
end


